@{
    ViewData["Title"] = "Accounts";
}

<h1>Accounts</h1>

<table>
    <thead>
        <tr>
            <th>Account ID</th>
            <th>Account Type</th>
            <th>Account Number</th>
            <th>Balance</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model)
        {
            <tr>
                <td>@account.Id</td>
                <td>@account.AccountType</td>
                <td>@account.AccountNumber</td>
                <td>@account.Balance</td>
            </tr>
        }
    </tbody>
</table>
