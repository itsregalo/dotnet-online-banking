@{
    ViewData["Title"] = "Payment History";
}

<h1>Payment History</h1>

<table>
    <thead>
        <tr>
            <th>Payment ID</th>
            <th>Customer ID</th>
            <th>Payee ID</th>
            <th>Payment Date</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var paymentHistory in Model)
        {
            <tr>
                <td>@paymentHistory.Id</td>
                <td>@paymentHistory.CustomerId</td>
                <td>@paymentHistory.PayeeId</td>
                <td>@paymentHistory.PaymentDate</td>
                <td>@paymentHistory.Amount</td>
            </tr>
        }
    </tbody>
</table>
